{
  "name": "@sissijs/muh",
  "description": "Muh (say: Moo!) stands for 'mustached hypertext' and is a template language for static site generators",
  "version": "2.0.0",
  "type": "module",
  "main": "src/muh.js",
  "exports": {
    ".": {
      "esm": "./dist/muh.js",
      "types": "./dist/muh.d.ts"
    },
    "./template": {
      "esm": "./dist/template.js",
      "types": "./dist/template.d.ts"
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "lint": "eslint src",
    "build": "rollup -c rollup.config.js",
    "postbuild": "npm run types -s",
    "types": "tsc",
    "docs": "typedoc src/muh.js src/typedefs.js src/builtin-filters.js src/builtin-helpers.js",
    "test": "node --test"
  },
  "keywords": [],
  "author": "Lea Rosema",
  "repository": "github:sissijs/muh",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "rollup": "^4.29.1",
    "typedoc": "^0.27.6",
    "typescript": "^5.7.2"
  }
}
